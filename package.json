{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "concurrently \"npm run build:css\" \"remix build\"",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "remix watch",
    "postinstall": "remix setup node",
    "start": "concurrently \"npm run dev\" \"cross-env NODE_ENV=production node server/index.js\"",
    "start:dev": "concurrently \"npm run dev:css\" \"npm run dev\" \"cross-env NODE_ENV=development node server/index.js\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "seed": "npx ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.8.1",
    "@remix-run/express": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "postcss": "^8.4.5",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
